{
  "name": "@jeeny/api",
  "version": "1.0.0",
  "description": "Jeeny API Services for GCP Cloud Run",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:auth": "tsc -p tsconfig.auth.json",
    "build:users": "tsc -p tsconfig.users.json",
    "build:rides": "tsc -p tsconfig.rides.json",
    "dev:auth": "ts-node-dev --respawn src/services/auth/index.ts",
    "dev:users": "ts-node-dev --respawn src/services/users/index.ts",
    "dev:rides": "ts-node-dev --respawn src/services/rides/index.ts",
    "start:auth": "node dist/services/auth/index.js",
    "start:users": "node dist/services/users/index.js",
    "start:rides": "node dist/services/rides/index.js",
    "lint": "eslint src --ext .ts",
    "test": "jest"
  },
  "dependencies": {
    "@google-cloud/functions-framework": "^3.3.0",
    "@google-cloud/pubsub": "^4.0.0",
    "@googlemaps/google-maps-services-js": "^3.3.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "firebase-admin": "^11.11.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.20",
    "@types/node": "^20.9.0",
    "@types/uuid": "^9.0.6",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
