{
  "name": "@gocap/driver",
  "version": "1.0.0",
  "private": true,
  "description": "Jeeny Driver Mobile Application - Taxi Driver App",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "dev": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:all": "eas build --platform all",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios",
    "update": "eas update",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rimraf node_modules .expo dist",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean"
  },
  "dependencies": {
    "@gocap/api": "1.0.0",
    "@gocap/shared": "1.0.0",
    "@gocap/ui": "1.0.0",
    "@aws-amplify/api": "^6.0.12",
    "@aws-amplify/auth": "^6.0.12",
    "@aws-amplify/geo": "^3.0.12",
    "@expo/vector-icons": "^14.0.0",
    "@gorhom/bottom-sheet": "^4.6.0",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-native-community/netinfo": "11.1.0",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.10",
    "@react-navigation/native-stack": "^6.9.18",
    "@react-navigation/stack": "^6.3.21",
    "@tanstack/react-query": "^5.17.0",
    "aws-amplify": "^6.0.12",
    "axios": "^1.6.2",
    "date-fns": "^3.2.0",
    "expo": "~50.0.0",
    "expo-av": "~13.10.0",
    "expo-background-fetch": "~11.6.0",
    "expo-camera": "~14.0.0",
    "expo-constants": "~15.4.0",
    "expo-dev-client": "~3.3.0",
    "expo-device": "~5.9.0",
    "expo-document-picker": "~11.10.0",
    "expo-file-system": "~16.0.0",
    "expo-font": "~11.10.0",
    "expo-haptics": "~12.8.0",
    "expo-image": "~1.10.0",
    "expo-image-picker": "~14.7.0",
    "expo-linear-gradient": "~12.7.0",
    "expo-linking": "~6.2.0",
    "expo-local-authentication": "~13.8.0",
    "expo-location": "~16.5.0",
    "expo-network": "~5.8.0",
    "expo-notifications": "~0.27.0",
    "expo-router": "~3.4.0",
    "expo-secure-store": "~12.8.0",
    "expo-splash-screen": "~0.26.0",
    "expo-status-bar": "~1.11.0",
    "expo-system-ui": "~2.9.0",
    "expo-task-manager": "~11.6.0",
    "expo-updates": "~0.24.0",
    "expo-web-browser": "~12.8.0",
    "i18next": "^23.7.16",
    "libphonenumber-js": "^1.10.54",
    "lottie-react-native": "6.5.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^14.0.0",
    "react-native": "0.73.2",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-maps": "1.10.0",
    "react-native-maps-directions": "^1.9.0",
    "react-native-mmkv": "^2.11.0",
    "react-native-pager-view": "6.2.3",
    "react-native-reanimated": "~3.6.1",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-svg": "14.1.0",
    "react-native-webview": "13.6.4",
    "zustand": "^4.4.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^29.5.11",
    "@types/react": "~18.2.45",
    "@types/react-native": "^0.73.0",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "eslint": "^8.56.0",
    "eslint-config-expo": "^7.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^29.7.0",
    "jest-expo": "~50.0.0",
    "prettier": "^3.1.1",
    "react-test-renderer": "18.2.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/*.test.{ts,tsx}",
      "!src/**/*.spec.{ts,tsx}"
    ]
  },
  "expo": {
    "name": "Jeeny Driver",
    "slug": "jeeny-driver",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "jeeny-driver",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1A1A2E"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "mr.jeeny.driver",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "يحتاج تطبيق جيني للسائقين إلى موقعك لعرض موقعك للركاب وتتبع الرحلات.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "يحتاج تطبيق جيني للسائقين إلى موقعك في الخلفية لتتبع الرحلات وإرسال موقعك للركاب.",
        "NSLocationAlwaysUsageDescription": "يحتاج تطبيق جيني للسائقين إلى موقعك في الخلفية لتتبع الرحلات.",
        "NSCameraUsageDescription": "يحتاج تطبيق جيني للسائقين إلى الكاميرا لتصوير المستندات والسيارة.",
        "NSPhotoLibraryUsageDescription": "يحتاج تطبيق جيني للسائقين إلى مكتبة الصور لتحميل المستندات.",
        "NSMicrophoneUsageDescription": "يحتاج تطبيق جيني للسائقين إلى الميكروفون للمكالمات مع الركاب.",
        "UIBackgroundModes": [
          "location",
          "fetch",
          "remote-notification"
        ],
        "CFBundleAllowMixedLocalizations": true,
        "CFBundleDevelopmentRegion": "ar",
        "CFBundleLocalizations": ["ar", "fr", "en"]
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1A1A2E"
      },
      "package": "mr.jeeny.driver",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "FOREGROUND_SERVICE",
        "FOREGROUND_SERVICE_LOCATION",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_MEDIA_IMAGES",
        "RECORD_AUDIO",
        "VIBRATE",
        "RECEIVE_BOOT_COMPLETED",
        "WAKE_LOCK",
        "POST_NOTIFICATIONS"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://driver.jeeny.mr"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "../../assets/fonts/Gilroy-Thin.ttf",
            "../../assets/fonts/Gilroy-Light.ttf",
            "../../assets/fonts/Gilroy-Regular.ttf",
            "../../assets/fonts/Gilroy-Medium.ttf",
            "../../assets/fonts/Gilroy-SemiBold.ttf",
            "../../assets/fonts/Gilroy-Bold.ttf",
            "../../assets/fonts/Gilroy-ExtraBold.ttf",
            "../../assets/fonts/Gilroy-Black.ttf"
          ]
        }
      ],
      "expo-localization",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "يسمح لتطبيق جيني للسائقين باستخدام موقعك لتتبع الرحلات وعرض موقعك للركاب.",
          "isAndroidBackgroundLocationEnabled": true,
          "isAndroidForegroundServiceEnabled": true
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "يسمح لتطبيق جيني للسائقين بالوصول إلى الكاميرا لتصوير المستندات."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "يسمح لتطبيق جيني للسائقين بالوصول إلى صورك لتحميل المستندات.",
          "cameraPermission": "يسمح لتطبيق جيني للسائقين بالوصول إلى الكاميرا لتصوير المستندات."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#FFD600",
          "defaultChannel": "default",
          "sounds": [
            "./assets/sounds/notification.wav",
            "./assets/sounds/ride_request.wav",
            "./assets/sounds/new_ride.wav"
          ]
        }
      ],
      [
        "expo-av",
        {
          "microphonePermission": "يسمح لتطبيق جيني للسائقين بالوصول إلى الميكروفون للمكالمات مع الركاب."
        }
      ],
      [
        "expo-task-manager",
        {}
      ],
      [
        "expo-background-fetch",
        {}
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "minSdkVersion": 24,
            "buildToolsVersion": "34.0.0",
            "kotlinVersion": "1.9.0",
            "enableProguardInReleaseBuilds": true,
            "enableShrinkResourcesInReleaseBuilds": true
          },
          "ios": {
            "deploymentTarget": "14.0",
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "your-driver-project-id-here"
      }
    },
    "owner": "jeeny",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/your-driver-project-id-here",
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 30000
    }
  }
}
