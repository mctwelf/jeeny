{
  "name": "@gocap/client",
  "version": "1.0.0",
  "private": true,
  "description": "GoCap Client Mobile Application - Taxi Booking App for Passengers",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "dev": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:all": "eas build --platform all",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios",
    "update": "eas update",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rimraf node_modules .expo dist",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean"
  },
  "dependencies": {
    "@gocap/api": "1.0.0",
    "@gocap/shared": "1.0.0",
    "@gocap/ui": "1.0.0",
    "@aws-amplify/api": "^6.0.12",
    "@aws-amplify/auth": "^6.0.12",
    "@aws-amplify/geo": "^3.0.12",
    "@expo/vector-icons": "^14.0.0",
    "@gorhom/bottom-sheet": "^4.6.0",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-native-community/netinfo": "11.1.0",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.10",
    "@react-navigation/native-stack": "^6.9.18",
    "@react-navigation/stack": "^6.3.21",
    "@tanstack/react-query": "^5.17.0",
    "aws-amplify": "^6.0.12",
    "axios": "^1.6.2",
    "date-fns": "^3.2.0",
    "expo": "~50.0.0",
    "expo-av": "~13.10.0",
    "expo-camera": "~14.0.0",
    "expo-constants": "~15.4.0",
    "expo-dev-client": "~3.3.0",
    "expo-device": "~5.9.0",
    "expo-file-system": "~16.0.0",
    "expo-font": "~11.10.0",
    "expo-haptics": "~12.8.0",
    "expo-image": "~1.10.0",
    "expo-image-picker": "~14.7.0",
    "expo-linear-gradient": "~12.7.0",
    "expo-linking": "~6.2.0",
    "expo-local-authentication": "~13.8.0",
    "expo-location": "~16.5.0",
    "expo-network": "~5.8.0",
    "expo-notifications": "~0.27.0",
    "expo-router": "~3.4.0",
    "expo-secure-store": "~12.8.0",
    "expo-splash-screen": "~0.26.0",
    "expo-status-bar": "~1.11.0",
    "expo-system-ui": "~2.9.0",
    "expo-updates": "~0.24.0",
    "expo-web-browser": "~12.8.0",
    "i18next": "^23.7.16",
    "libphonenumber-js": "^1.10.54",
    "lottie-react-native": "6.5.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^14.0.0",
    "react-native": "0.73.2",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-maps": "1.10.0",
    "react-native-mmkv": "^2.11.0",
    "react-native-pager-view": "6.2.3",
    "react-native-reanimated": "~3.6.1",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-svg": "14.1.0",
    "react-native-webview": "13.6.4",
    "zustand": "^4.4.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^29.5.11",
    "@types/react": "~18.2.45",
    "@types/react-native": "^0.73.0",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "eslint": "^8.56.0",
    "eslint-config-expo": "^7.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^29.7.0",
    "jest-expo": "~50.0.0",
    "prettier": "^3.1.1",
    "react-test-renderer": "18.2.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/*.test.{ts,tsx}",
      "!src/**/*.spec.{ts,tsx}"
    ]
  },
  "expo": {
    "name": "GoCap",
    "slug": "gocap-client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "gocap",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFD600"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "mr.gocap.client",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "GoCap needs your location to find nearby drivers and show your position on the map.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "GoCap needs your location to track your ride and provide accurate navigation.",
        "NSCameraUsageDescription": "GoCap needs camera access to take profile photos and scan documents.",
        "NSPhotoLibraryUsageDescription": "GoCap needs photo library access to upload profile photos.",
        "NSMicrophoneUsageDescription": "GoCap needs microphone access for in-app calls with drivers."
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFD600"
      },
      "package": "mr.gocap.client",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "RECORD_AUDIO",
        "VIBRATE",
        "RECEIVE_BOOT_COMPLETED"
      ],
      "config": {
        "googleMaps": {
          "apiKey": ""
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-localization",
      "expo-secure-store",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow GoCap to use your location to track rides and find nearby drivers."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow GoCap to access your camera to take profile photos."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow GoCap to access your photos to upload profile pictures."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#FFD600",
          "sounds": [
            "./assets/sounds/notification.wav"
          ]
        }
      ],
      [
        "expo-av",
        {
          "microphonePermission": "Allow GoCap to access your microphone for in-app calls."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": ""
      }
    },
    "owner": "gocap"
  }
}
