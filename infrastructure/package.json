{
  "name": "@gocap/infrastructure",
  "version": "1.0.0",
  "private": true,
  "description": "GoCap AWS Infrastructure - CDK Stack for taxi booking platform",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "synth": "cdk synth",
    "diff": "cdk diff",
    "deploy": "cdk deploy --all",
    "deploy:dev": "cdk deploy --all --context environment=dev",
    "deploy:staging": "cdk deploy --all --context environment=staging",
    "deploy:prod": "cdk deploy --all --context environment=prod --require-approval broadening",
    "destroy": "cdk destroy --all",
    "bootstrap": "cdk bootstrap",
    "lint": "eslint lib --ext .ts",
    "lint:fix": "eslint lib --ext .ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rimraf cdk.out lib *.js *.d.ts"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.120.0",
    "constructs": "^10.3.0",
    "@aws-cdk/aws-cognito-identitypool-alpha": "^2.120.0-alpha.0",
    "@aws-cdk/aws-location-alpha": "^2.120.0-alpha.0",
    "source-map-support": "^0.5.21",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "aws-cdk": "^2.120.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.100.0",
    "constructs": "^10.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "gocap",
    "aws",
    "cdk",
    "infrastructure",
    "serverless",
    "taxi",
    "booking"
  ],
  "author": "GoCap Team",
  "license": "UNLICENSED"
}
